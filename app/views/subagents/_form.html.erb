<%= form_with(model: [@masteragent, @agent, @subagent], local: true) do |form| %>
  <% if subagent.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(subagent.errors.count, "error") %> prohibited this subagent from being saved:</h2>

      <ul>
      <% subagent.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  <div class="col-lg-12">
        <div class="ibox float-e-margins">
            <div class="ibox-title">
                <h5>Update Subagent</h5>

            </div>
            <div class="ibox-content">
                <div class="form-horizontal">
                  <div class="form-group row"><label class="col-sm-2 col-form-label">Shop Name</label>
                    <div class="col-sm-10">  <%= form.text_field :name, autocomplete: "name", class: 'form-control', placeholder: "Master Agent's store" %></div>
                 </div>
                      <div class="form-group">
                        <div class="col-lg-offset-2 col-lg-10">
                            <button class="btn btn-block btn-outline btn-primary" type="submit">Enter</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
<% end %>
